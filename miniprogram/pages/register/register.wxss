.container {
  padding: 40rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.title {
  font-size: 48rpx;
  font-weight: bold;
  margin-bottom: 60rpx;
}
.input {
  width: 80%;
  margin-bottom: 30rpx;
  padding: 20rpx;
  border: 1px solid #ddd;
  border-radius: 8rpx;
}
.btn {
  width: 80%;
  background: #52c41a;
  color: #fff;
  margin-bottom: 20rpx;
} 